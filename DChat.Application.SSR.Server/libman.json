{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "squirrelly@9.0.0",
      "destination": "wwwroot/squirrelly/",
      "files": [
        "dist/browser/squirrelly.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@8.0.0",
      "destination": "wwwroot/signalr/",
      "files": [
        "dist/browser/signalr.min.js"
      ]
    }
  ,
{
  "provider": "unpkg",
  "library": "jquery@3.7.1",
  "destination": "wwwroot/jquery/",
  "files": [
    "dist/jquery.slim.min.js"
  ]
}
]
}